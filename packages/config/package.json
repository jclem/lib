{
  "name": "@jclem/config",
  "version": "5.0.1",
  "description": "A configuration library for JavaScript runtimes",
  "license": "MIT",
  "author": "Jonathan Clem <j@jclem.me> (https://jclem.me)",
  "homepage": "https://github.com/jclem/lib/tree/main/packages/config",
  "keywords": [
    "configuration",
    "zod"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "packages/config",
    "type": "git",
    "url": "https://github.com/jclem/lib.git"
  },
  "bugs": "https://github.com/jclem/lib/issues",
  "exports": {
    ".": {
      "import": "./dist/esm/config.js",
      "require": "./dist/cjs/config.js"
    }
  },
  "types": "./dist/esm/config.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "rm -r dist; konk run c -cgL -n'build:*'",
    "build:esm": "tsc --outDir dist/esm --module esnext",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs --moduleResolution node --verbatimModuleSyntax false --declaration false"
  },
  "trustedDependencies": [
    "konk"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "konk": "^0.27.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "zod": "^3.22.4"
  }
}
