{
  "name": "@jclem/cli",
  "version": "1.0.1",
  "description": "A utility for parsing command-line arguments",
  "license": "MIT",
  "author": "Jonathan Clem <j@jclem.me> (https://jclem.me)",
  "homepage": "https://github.com/jclem/lib/tree/main/packages/cli",
  "keywords": [
    "cli"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "packages/cli",
    "type": "git",
    "url": "https://github.com/jclem/lib.git"
  },
  "bugs": "https://github.com/jclem/lib/issues",
  "exports": {
    ".": {
      "import": "./dist/esm/cli.js",
      "require": "./dist/cjs/cli.js"
    }
  },
  "types": "./dist/esm/cli.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "konk run c -cgL -n'build:*'",
    "build:esm": "tsc --outDir dist/esm --module esnext",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs --moduleResolution node --verbatimModuleSyntax false --declaration false"
  },
  "trustedDependencies": [
    "konk"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "konk": "^0.27.1",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
